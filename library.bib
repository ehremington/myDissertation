issn = {14779226},
journal = {Dalton Transactions},
number = {19},
pages = {3061--3066},
title = {{Doping and defect association in AZrO\textsubscript{3} (A = Ca, Ba) and LaMO\textsubscript{3} (M = Sc, Ga) perovskite-type ionic conductors}},
volume = {3},
year = {2004}
