file = {:home/eric/Documents/Mendeley/2018/Hossain et al. - Highly dense and chemically stable proton conducting electrolyte sintered at 1200°C.pdf:pdf},
issn = {03603199},
journal = {International Journal of Hydrogen Energy},
keywords = {Chemical stability,Electrolyte,Impedance analysis,Perovskite,Proton conductor,Rietveld refinement},
number = {2},
pages = {894--907},
publisher = {Elsevier Ltd},
title = {{Highly dense and chemically stable proton conducting electrolyte sintered at 1200°C}},
url = {https://doi.org/10.1016/j.ijhydene.2017.11.111},
volume = {43},
year = {2018}
