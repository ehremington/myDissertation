doi = {10.1039/b612060c},
file = {:home/eric/Documents/Mendeley/2008/Brett et al. - Intermediate temperature solid oxide fuel cells.pdf:pdf},
issn = {0306-0012},
journal = {Chemical Society Reviews},
keywords = {ANODE MATERIALS ION CONDUCTORS ZEBRA BATTERY ELECT},
pages = {1568--1578},
