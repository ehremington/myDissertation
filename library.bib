keywords = {energy discovery platforms,humidity,nanostructured ceria,proton conduction,tr-KPFM},
number = {34},
pages = {1--11},
publisher = {IOP Publishing},
title = {{Electrochemical reactivity and proton transport mechanisms in nanostructured ceria}},
