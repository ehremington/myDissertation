pages = {275--281},
title = {{Controlling crystal quality and orientation of pulsed-laser-deposited BaTiO 3 thin films by the kinetic energy of the film-forming particles}},
url = {http://link.springer.com/10.1007/s003390050047},
volume = {70},
year = {2000}
}
