journal = {Surface and Coatings Technology},
keywords = {Alumina thin films Optical coatings Polymers Prote},
pages = {1189----1194 ST ---- Pulsed laser deposition of alumin},
title = {{Pulsed laser deposition of alumina and zirconia thin films on polymers and glass as optical and protective coatings}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0033343445&partnerID=40&md5=994a3549a40d299865c38112523b14d3},
