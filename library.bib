pages = {4370--4387},
title = {{Oxide-ion and proton conducting electrolyte materials for clean energy applications: Structural and mechanistic features}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-77954773664&partnerID=40&md5=132b461478b8e023ac1642e3ac2175b8},
