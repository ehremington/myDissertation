}
@article{Giaquinta1994,
abstract = {Several approaches for predicting the structures of AB03 compounds are discussed and contrasted. The basic principles behind structure-composition predictions are reviewed, and a new approach for structural prediction that relies on the combination of ionic radii and bond ionicities is introduced. The major AB03 structures are described, and the criteria that govern their formation for different elements are examined. Two structural phase diagrams for AB03 compounds are analyzed and a new transition metal structure type, InMO3 (M = Mn, Fe), is explained within the context of the AB03 structural phase diagrams. Several compounds with the h M 0 3 structure are predicted.},
