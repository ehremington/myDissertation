file = {:home/eric/Documents/Mendeley/2008/Soboleva et al. - Investigation of the through-plane impedance technique for evaluation of anisotropy of proton conducting polymer membr.pdf:pdf},
issn = {15726657},
journal = {Journal of Electroanalytical Chemistry},
keywords = {AC impedance,Anisotropy,PEM,Through-plane conductivity},
month = {oct},
number = {2},
pages = {145--152},
title = {{Investigation of the through-plane impedance technique for evaluation of anisotropy of proton conducting polymer membranes}},
url = {http://www.sciencedirect.com/science/article/pii/S0022072808002301},
volume = {622},
year = {2008}
}
