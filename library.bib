author = {Kreuer, Klaus-Dieter and Adams, St and M{\"{u}}nch, W. and Fuchs, A. and Klock, U. and Maier, Joachim},
doi = {10.1016/s0167-2738(01)00953-5},
file = {:home/eric/Documents/Mendeley/2001/Kreuer et al. - Proton conducting alkaline earth zirconates and titanates for high drain electrochemical applications.pdf:pdf},
