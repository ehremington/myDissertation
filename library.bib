isbn = {0167-2738},
issn = {01672738},
journal = {Solid State Ionics},
month = {jul},
number = {PART 2},
pages = {998--1003},
title = {{Proton conductive properties of gadolinium-doped barium cerates at high temperatures}},
url = {http://linkinghub.elsevier.com/retrieve/pii/016727389290283U},
volume = {53-56},
year = {1992}
