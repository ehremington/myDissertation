file = {:home/eric/Documents/Mendeley/1999/Fleig, Maier - The impedance of ceramics with highly resistive grain boundaries validity and limits of the brick layer model.pdf:pdf},
isbn = {7116891722},
issn = {09552219},
journal = {Journal of the European Ceramic Society},
keywords = {brick layer model,electrical properties,grain boundaries,impedance,microstructure},
number = {6-7},
pages = {693--696},
title = {{The impedance of ceramics with highly resistive grain boundaries: validity and limits of the brick layer model}},
volume = {19},
year = {1999}
