publisher = {Elsevier},
title = {{Tailoring the chemical stability of Ba(Ce0.8 - xZrx)Y0.2O3 - $\delta$protonic conductors for Intermediate Temperature Solid Oxide Fuel Cells (IT-SOFCs)}},
url = {http://cat.inist.fr/?aModele=afficheN&cpsidt=20396601 http://linkinghub.elsevier.com/retrieve/pii/S0167273808003275},
