issn = {19448252},
journal = {ACS Applied Materials and Interfaces},
keywords = {Kelvin probe force microscopy,atom probe tomography,density functional theory,proton-conducting solid oxide fuel cells,scanning transmission electron microscopy,yttrium-doped barium zirconate},
number = {5},
pages = {4816--4823},
title = {{Influence of Nonstoichiometry on Proton Conductivity in Thin-Film Yttrium-Doped Barium Zirconate}},
volume = {10},
year = {2018}
