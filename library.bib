@article{Mogensen2004,
abstract = {Many metal oxides of fluorite and perovskite related structures are oxide ion conductors, which have practical applications in devices such as oxygen sensors, solid oxide fuel cells (SOFC) and electrolysers. Several structural and thermodynamic parameters such as (1) critical radius of the pathway for the oxide ion movement, (2) free lattice volume, and (3) average metal-oxide bond energy have been proposed as predictors of high oxide ion conductivity. We discuss how these parameters all depend on ionic radii, and therefore, some of these may be redundant. Furthermore, we explore the interrelations among such parameters for fluorite and perovskite oxides by considering their sensitivities to the individual ionic radii. Based on experimental data available in the literature, it is argued that lattice distortion (lattice stress and deviation from cubic symmetry) due to ion radii mismatch determines the ionic conductivity to a very large extent, and that lattice distortion is of much greater importance than many other proposed parameters. In case of the perovskites, the charge of the B-site ion is also of major importance. {\textcopyright} 2004 Published by Elsevier B.V.},
author = {Mogensen, Mogens and Lybye, D. and Bonanos, N. and Hendriksen, P. V. and Poulsen, F. W.},
doi = {10.1016/j.ssi.2004.07.036},
file = {:home/eric/Documents/Mendeley/2004/Mogensen et al. - Factors controlling the oxide ion conductivity of fluorite and perovskite structured oxides.pdf:pdf},
isbn = {0167-2738},
