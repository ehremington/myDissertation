doi = {10.1021/cm200682d},
file = {:home/eric/Documents/Mendeley/2011/Giannici et al. - Long-Range and Short-Range Structure of Proton-Conducting YBaZrOtextsubscript{3}.pdf:pdf},
issn = {0897-4756},
journal = {Chemistry of Materials},
keywords = {barium zirconate,exafs,proton conductor,raman,xrd},
